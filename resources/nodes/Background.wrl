# The Background node defines the background used for rendering the 3D world.

Background {
  vrmlField MFColor  skyColor  [ 0 0 0 ]
  field     SFNode   cubemap   NULL
  deprecatedField MFString backUrl   []
  deprecatedField MFString bottomUrl []
  deprecatedField MFString frontUrl  []
  deprecatedField MFString leftUrl   []
  deprecatedField MFString rightUrl  []
  deprecatedField MFString topUrl    []
}
